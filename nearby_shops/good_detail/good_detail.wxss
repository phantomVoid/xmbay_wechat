  z-index: 50;
  display: flex;
  width: 100%;
  height: 100rpx;
  overflow: hidden;
  background: white;
  box-shadow: 0 0 10rpx 0 rgba(155, 155, 155, 0.3);
}

.wrap-footer .f-left-con {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 42%;
