.hot-classify-list {
  position: fixed;
  right: 160rpx;
  bottom: 130rpx;
  z-index: 2;
  width: 245rpx;
  border-radius: 8rpx;
  border: 1px solid #f6f6f6;
  background: white;
}

.hot-classify-list view {
  text-align: center;
  font-size: 30rpx;
  line-height: 94rpx;
  border-bottom: 1px solid #f6f6f6;
}

.hot-classify-list view:last-child {
  border-bottom: 1px solid white;
}

.hot-classify-list::before {
  content: "";
  position: absolute;
  left: 50%;
  bottom: -16rpx;
  width: 0;
  height: 0;
  margin-left: -18rpx;
  border-left: solid 18rpx transparent;
  border-top: solid 16rpx #e3e3e3;
  border-right: solid 18rpx transparent;
}

.hot-classify-list::after {
  content: "";
  width: 0;
  height: 0;
  position: absolute;
  left: 50%;
  margin-left: -16rpx;
  bottom: -14rpx;
  border-left: solid 16rpx transparent;
  border-top: solid 14rpx white;
  border-right: solid 16rpx transparent;
}

