.shop-search .attention {
  position: absolute;
  right: 0;
  bottom: 4rpx;
  z-index: 2;
}

.shop-search .attention  image {
  width: 116rpx;
  height: 82rpx;
}

.shop-search .attention view {
  position: absolute;
  width: 100%;
  left: 4rpx;
  padding-top: 4rpx;
  bottom: 14rpx;
  font-size: 22rpx;
  text-align: center;
  color: white;
}

.shop-footer {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 15;
  width: 100%;
  height: 100rpx;
  overflow: hidden;
  background: white;
  border-top: 1px solid #f0f0f0;
}

.shop-footer .shop-operation {
  width: 83%;
  float: left;
  overflow: hidden;
}

.shop-footer .shop-service {
  width: 17%;
  float: left;
  text-align: center;
}

.shop-footer .shop-operation > view {
  width: 50%;
  float: left;
  height: 100%;
  line-height: 100rpx;
  text-align: center;
  font-size: 28rpx;
  position: relative;
}

.shop-footer .shop-detail::after {
  content: "";
  display: block;
  position: absolute;
  top: 50%;
  right: 0;
  width: 2rpx;
  height: 42rpx;
  margin-top: -21rpx;
  background: #f0f0f0;
}

.shop-footer .hot-classify::after {
  content: "";
  display: block;
  position: absolute;
  top: 0;
  right: 0;
  width: 2rpx;
  height: 100%;
  background: #f0f0f0;
}

.shop-footer .hot-classify .line-group {
  position: absolute;
  top: 8rpx;
  left: 50%;
  margin-left: -18rpx;
}

.shop-footer .hot-classify .line-group > view {
  width: 36rpx;
  height: 4rpx;
  margin-top: 6rpx;
  background: #e3e3e3;
}

.shop-footer .shop-service image {
  width: 42rpx;
  height: 42rpx;
  padding-top: 14rpx;
  display: block;
  margin: 0 auto;
}

.shop-footer .shop-service view {
  font-size: 20rpx;
}

.shop-tab {
  position: relative;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  overflow: hidden;
  background: white;
  border-bottom: 1px solid #f6f6f6;
}

.shop-tab > view {
  width: 25%;
  text-align: center;
  float: left;
}

.shop-tab > view text {
  font-size: 30rpx;
  position: relative;
  line-height: 100rpx;
  height: 100rpx;
  display: inline-block;
  border-bottom: 5rpx solid #fff;
}

.shop-tab > view text::after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 4rpx;
  background: white;
}

