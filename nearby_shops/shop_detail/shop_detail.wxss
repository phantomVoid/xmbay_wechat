.board {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.shop-search {
  padding: 20rpx 25rpx 30rpx 23rpx;
  background: white;
  overflow: hidden;
  position: relative;
}

.shop-search .shop-search-bg {
  position: relative;
  z-index: 2;
  width: 630rpx;
  height: 64rpx;
  border-radius: 8rpx;
  float: left;
  display: flex;
  align-items: center;
  overflow: hidden;
  background: #f0f2f5;
  background: rgba(255, 255, 255, 0.3);
}

.shop-search .shop-search-bg input {
  width: 520rpx;
  height: 25rpx;
  font-size: 25rpx;
  margin-left: 25rpx;
  float: left;
  color: white;
}

.shop-search-bg .icon {
  float: left;
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}

.shop-search-head {
  position: relative;
  z-index: 2;
  overflow: hidden;
  display: flex;
  align-items: center;
}

.shop-search-head .operation-icon {
  width: 58rpx;
  height: 58rpx;
  float: left;
  margin-left: 14rpx;
}

.shop-search > image {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
}

.shop-search >.shade {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  height: 100%;
  background: linear-gradient(to top, rgba(0, 0, 0, 0), rgba(0, 0, 0, 0.5));
  /* background-color: rgba(0, 0, 0, 0.3); */
}

.shop-info {
  position: relative;
  z-index: 2;
  width: 500rpx;
  overflow: hidden;
  padding-top: 44rpx;
}

.shop-info > .store_logo {
  width: 70rpx;
  height: 70rpx;
  float: left;
}
.shop-info > .store_logo>image{
  width: 100%;
  height: 100%;
}

.shop-info > view {
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  height: 70rpx;
  float: left;
  margin-left: 10rpx;
  color: white;
}

.shop-info > view > view {
  overflow: hidden;
}

.shop-info .shop-name {
  font-size: 26rpx;
  line-height: 26rpx;
}

.shop-info .shop-classify {
  width: 112rpx;
  height: 32rpx;
}

