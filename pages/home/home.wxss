.wrap {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100vh;
}

.wrap .header {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 20;
  display: flex;
  align-items: center;
  flex-shrink: 0;
  width: 100%;
  height: 100rpx;
  padding: 0 24rpx;
