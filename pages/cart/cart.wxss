page {
  height: 100%;
  background: white;
}

.warp1 {
  display: flex;
  flex-direction: column;
  height: calc(100vh - 100rpx);
}

.trans {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  background: rgb(255, 255, 255, 0);
}

.login {
  width: 100%;
  height: 84rpx;
  display: flex;
  align-items: center;
  color: #64676c;
  background: white;
}

.login > view:first-child {
  width: 144rpx;
  height: 48rpx;
  font-size: 26rpx;
  line-height: 48rpx;
  border-radius: 30rpx;
  text-align: center;
  margin: 0 30rpx;
  border: 2rpx solid #f0f0f0;
}

.login > view:last-child {
  font-size: 26rpx;
}

.warp1 .scroll-view {
  flex: 1;
  height: calc(100vh - 210rpx);
}

.cart-empty {
  padding-bottom: 60rpx;
}

.cart-empty image {
  width: 100%;
  height: 186rpx;
}

.cart-empty > view {
  display: flex;
  justify-content: center;
  width: 100%;
}

.cart-empty > view > view {
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  text-align: center;
  border-radius: 50rpx;
  border: 2rpx solid #e6e6e6;
}

.cart-empty > view > view:nth-child(2) {
  margin-left: 40rpx;
}

.cart .shop {
  position: relative;
  display: flex;
  height: 90rpx;
  background: #f8f8f8;
}

.cart .shop .select {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 88rpx;
  height: 90rpx;
}

.cart .shop .select .imager {
  font-size: 40rpx;
  color: #9ea3a9;
}

.cart .shop-name {
  height: 100%;
  display: flex;
  align-items: center;
}

.cart .shop-name .shop-icon {
  width: 32rpx;
  height: 32rpx;
}

.cart .shop-name > view {
  font-size: 30rpx;
  line-height: 32rpx;
  margin-left: 14rpx;
}

.cart .shop-name .next {
  width: 12rpx;
  height: 22rpx;
  margin-left: 22rpx;
}

.cart .shop .coupon {
  position: absolute;
  right: 24rpx;
  top: 0;
  font-size: 28rpx;
  line-height: 90rpx;
}

.cart .discount {
  position: relative;
  display: flex;
  align-items: center;
  margin-left: 88rpx;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}

.cart .discount image {
  width: 66rpx;
  height: 32rpx;
}

.cart .discount .discount-info {
  font-size: 26rpx;
  margin-left: 12rpx;
}

.cart .discount .go-discount {
  position: absolute;
  right: 24rpx;
  top: 0;
  font-size: 26rpx;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}

.cart .good {
  position: relative;
  display: flex;
  width: 100%;
  height: 246rpx;
}

.cart .good  .good-board {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 12;
  display: flex;
  align-items: center;
  justify-content: center;
}

.cart .good  .good-board > view {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 90rpx;
  width: 90rpx;
  font-size: 28rpx;
  line-height: 36rpx;
  padding: 20rpx;
  border-radius: 100%;
  margin-left: 32rpx;
  color: white;
}

.cart .good  .good-board > view text {
  width: 56rpx;
  text-align: center;
}

.cart .good  .good-board .collect {
  margin-left: 0;
  background: #e5e5e5;
  color: black;
}

.cart .good  .good-board .edit {
  background: #ff8c4d;
}

.cart .good  .good-board .del {
  background: #ed2323;
}

.cart .good .select {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 88rpx;
  height: 100%;
}

.cart .good .select .imager {
  font-size: 40rpx;
  color: #9ea3a9;
}

.cart .good .good-wrap {
  flex: 1;
  padding: 23rpx 0;
}

.cart .good .good-info {
  display: flex;
  height: 200rpx;
}

.cart .good .good-avatar {
  flex-shrink: 0;
  width: 200rpx;
  height: 200rpx;
}
.cart .good .good-avatar .file{
  width: 100%;
  height: 100%;
}

.cart .good .good-info > .good-con {
  position: relative;
  flex: 1;
  height: 100%;
  margin-left: 20rpx;
  overflow: hidden;
}

.cart .good .good-info .good-name {
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-right: 24rpx;
}

.cart .good .good-info .good-attr {
  display: inline-flex;
  align-items: center;
  font-size: 25rpx;
  margin-top: 15rpx;
  padding: 4rpx 8rpx;
  color: #999;
  background-color: #f6f6f6;
}

.cart .good .good-info .good-attr .attr {
  width: 378rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.cart .good .good-info .good-attr .imager {
  display: inline-block;
  font-size: 24rpx;
}

.cart .good .good-info .good-operation {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
}

.cart .good .good-info .good-operation .price {
  overflow: hidden;
  display: inline-block;
}

.good-info .good-operation .price .unit {
  font-size: 24rpx;
}

.good-info .good-operation .price .integer {
  font-size: 30rpx;
}

.good-info .good-operation .price .decimals {
  font-size: 24rpx;
}

.cart .good .good-info .good-operation .good-num {
  overflow: hidden;
  border: 2rpx solid #ddd;
  margin-right: 24rpx;
  float: right;
  border-radius: 6rpx;
}

.cart .good .good-info .good-operation .good-num > image {
  float: left;
  width: 46rpx;
  height: 44rpx;
}

.cart .good .good-info .good-operation .good-num view {
  width: 68rpx;
  float: left;
  text-align: center;
  font-size: 24rpx;
  line-height: 44rpx;
  border-left: 2rpx solid #ddd;
  border-right: 2rpx solid #ddd;
}

.guess-like {
  background: #f6f6f6;
  padding-bottom: 20rpx;
}

.guess-like .head {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 68rpx;
}

.guess-like .head image {
  width: 40rpx;
  height: 10rpx;
}

.guess-like .head text {
  font-size: 28rpx;
  font-weight: bold;
  margin: 0 20rpx;
}

.footer {
  display: flex;
  width: 100%;
  height: 100rpx;
  border-top: 1px solid rgba(155, 155, 155, 0.1);
  background: white;
}

.footer .account {
  width: 185rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  text-align: center;
  color: white;
}

.footer-wrap {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.footer-wrap .select {
  display: flex;
  height: 100rpx;
  align-items: center;
  padding-left: 24rpx;
  padding-right: 10rpx;
}

.footer-wrap .select .imager {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 40rpx;
  color: #9ea3a9;
}

.footer-wrap .select > view {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  margin-left: 10rpx;
  color: #4f4f4f;
}

.footer-wrap .total-price {
  height: 100rpx;
  padding-right: 10rpx;
}

.footer-wrap .total-price .total {
  text-align: right;
  margin-top: 34rpx;
}

.footer-wrap .total-price .total > view {
  line-height: 28rpx;
}

.footer-wrap .total-price text {
  font-size: 28rpx;
}

.footer-wrap .total-price .onsale {
  margin-top: 20rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #696f73;
}

.lose-efficacy {
  margin-top: 10rpx;
}

.lose-efficacy-head {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  line-height: 90rpx;
  padding: 0 24rpx;
}

.lose-efficacy .select > view {
  font-size: 26rpx;
  line-height: 34rpx;
  width: 70rpx;
  text-align: center;
  border-radius: 20rpx;
  color: white;
  background: #aaa;
}

.lose-efficacy .good-lose-efficacy {
  font-size: 26rpx;
  margin-top: 80rpx;
}

.rechoose {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
  width: 100%;
  margin-top: 50rpx;
}

.rechoose > view:last-child {
  font-size: 28rpx;
  line-height: 48rpx;
  width: 100rpx;
  text-align: center;
  margin-right: 26rpx;
  border-radius: 30rpx;
}

.buy {
  padding-bottom: 100rpx;
}

.gwc-wntj {
  width: 40rpx;
  height: 10rpx;
}

.gwc-wntj>view {
  border-radius: 50%;
  overflow: hidden;
}

.gwc-wntj>view:nth-child(1) {
  widows: 5px;
  height: 5px;
}

