.wrap{
  display: flex;
  flex-direction: column;
  width: 100%;
  overflow: hidden;
}
.wrap >view{
  width: 100%;
}
.wrap .compile{
  width: 100%;
  height: 100rpx;
}
.wrap .compile .con{
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-top: 1px solid #f6f6f6;
  border-bottom: 1px solid #f6f6f6;
}
.wrap .compile .con .tab-con{
  display: flex;
  align-items: center;
  width: 70%;
  font-size: 28rpx;
  color: #999;
}
.wrap .compile .con .tab-con .icon{
  font-size: 32rpx;
  margin-right: 10rpx;
  color: #aaa;
}
.wrap .compile .con .tab-con .title{
  width: 100%;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.wrap .compile .con .tab-con .title text{
  font-size: 30rpx;
  margin: 0 4rpx;
}
.wrap .compile .compile-btn,.accomplish-btn{
  display: flex;
  align-items: center;
  flex-direction: row-reverse;
  width: 80rpx;
  height: 100%;
  text-align: right;
  margin-left: auto;
}

.wrap .cart-con .login-con{
  display: flex;
  align-items: center;
  width: 100%;
  padding: 20rpx 30rpx 0;
  box-sizing: border-box;
  background-color: #fff;
}
.wrap .cart-con .login-con .btn{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 145rpx;
  height: 50rpx;
  margin-right: 24rpx;
  border-radius: 50px 50px;
  border: 1px solid #ececec;
  overflow: hidden;
}
.wrap .cart-con .login-con view{
  font-size: 24rpx;
  color: #5f6367;
}
.wrap .cart-con .cart-empty-wrap{
  background-color: #fff;
}
.wrap .cart-con .cart-empty-wrap image{
  width: 100%;
  height: 190rpx;
}
.wrap .cart-con .cart-empty-wrap .row{
  display: flex;
  justify-content: center;
  width: 100%;
  padding-bottom: 40rpx;
}
.wrap .cart-con .cart-empty-wrap .row view{
  font-size: 28rpx;
  padding: 10rpx 35rpx;
  border: 1px solid #e0dfdf;
  border-radius: 50px 50px;
  color: #333;
}
.wrap .cart-con .cart-list{}
.wrap .cart-con .cart-list .cart-item{
  margin-bottom: 18rpx;
  border-top:1px solid #efefef;
}
.wrap .cart-con .cart-list .cart-item .shop-con{
  display: flex;
  align-items: center;
  width: 100%;
  height: 90rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #f8f8f8;
}
.wrap .cart-con .shop-con .shop-wrap{
  display: flex;
  align-items: center;
  margin-left: 22rpx;
  color: #333;
}
.wrap .cart-con .shop-con .shop-wrap .icon-xq-dp{
  font-size: 36rpx;
  margin-right: 14rpx;
}
.wrap .cart-con .shop-con .shop-wrap .shop-name{
  font-size: 28rpx;
}
.wrap .cart-con .shop-con .shop-wrap .icon-next{
  font-size: 20rpx;
  margin-left: 10rpx;
  color: #bdbaba;
}
.wrap .cart-con .shop-con .coupon{
  font-size: 26rpx;
  margin-left: auto;
  color: #333;
}
.wrap .cart-con .goods-list{
  width: 100vw;
}
.wrap .cart-con .goods-list .goods-item{
  display: flex;
  align-items: center;
  width: 100%;
  height: 250rpx;
  padding-left: 30rpx;
  font-size: 26rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom:1px solid #efefef;
}
.wrap .cart-con .goods-list .goods-item .goods-file{
  flex-shrink: 0;
  width: 200rpx;
  height: 200rpx;
  margin-left: 25rpx;
}
.wrap .cart-con .goods-list .goods-item .goods-file image{
  width: 100%;
  height: 100%;
}
.wrap .cart-con .goods-list .goods-item .goods-con{
  display: flex;
  flex-direction: column;
  flex: 1;
  height: 200rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
}

.wrap .cart-con .goods-list .goods-item .goods-con .goods-name{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  height: 84rpx;
  font-size: 28rpx;
  line-height: 38rpx;
  color: #333;
  overflow: hidden;
}

.wrap .cart-con .goods-list .goods-item .goods-con .goods-attr{
  display: flex;
  align-items: center;
  font-size: 22rpx;
  line-height: 38rpx;
  border-radius: 6rpx;
  padding: 4rpx 10rpx;
  box-sizing: border-box;
  background-color: #f9f9f9;
  color: #6e7479;
  overflow: hidden;
}
.wrap .cart-con .goods-list .goods-attr .attr{
  width: 378rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-right: auto;
  overflow: hidden;
}
.wrap .cart-con .goods-list .goods-attr .icon-sy-xl{
  font-size: 20rpx;
}
.wrap .cart-con .goods-list .goods-item .goods-con .goods-info{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: auto;
}
.wrap .goods-item .goods-con .goods-info .price{}
.wrap .goods-item .goods-con .goods-info .good-num{
  display: inline-flex;
  align-items: center;
  border-radius: 6rpx;
  border: 1px solid #e7e7e7;
  overflow: hidden;
}
.wrap .goods-item .goods-con .goods-info .good-num image{
  width: 46rpx;
  height: 44rpx;
}
.wrap .goods-item .goods-con .goods-info .good-num view{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 60rpx;
  height: 44rpx;
  font-size: 22rpx;
  border-left: 1px solid #e7e7e7;
  border-right: 1px solid #e7e7e7;
}
.wrap .cart-con .goods-con .rechoose{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  margin-top: auto;
  color: #333;
}
.wrap .cart-con .goods-con .rechoose .tip{
}
.wrap .cart-con .goods-con .rechoose .btn{
  display: inline-block;
  padding: 4rpx 15rpx;
  border: 1px solid transparent;
  border-radius: 50px 50px;
}
.wrap .cart-con .cart-empty{
  background-color: #fff;
}
.wrap .cart-con .cart-empty .icon{
  padding: 4rpx 10rpx;
  border-radius: 50px 50px;
  background-color: #aaa;
  color: #fff;
  overflow: hidden;
}
.wrap .cart-con .cart-empty .lose-efficacy-head{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 90rpx;
  font-size: 28rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  color: #333;
}
.wrap .cart-con .cart-empty .goods-name{
  color: #999!important;
}

.wrap .recommend-con {
  padding: 0 10rpx 20rpx;
  box-sizing: border-box;
  background: #f6f6f6;
}

.wrap .recommend-con .head {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 68rpx;
}

.wrap .recommend-con .head image {
  width: 40rpx;
  height: 10rpx;
}

.wrap .recommend-con .head text {
  font-size: 28rpx;
  font-weight: bold;
  margin: 0 20rpx;
}

.wrap .editor-con{
  width: 100%;
  height: 100rpx;
}

.wrap .editor-wrap{
  position: fixed;
  left: 0;
  bottom: 100rpx;
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  padding: 0 0 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-top: 1px solid rgba(155, 155, 155, .1);
  color: #4d4d4d;
}
.wrap .select{
  display: flex;
  align-items: center;
}
.wrap .select .icon{
  display: flex;
  align-items: center;
  height: 100%;
  font-size: 40rpx;
  color: #9399a0;
}
.wrap .editor-con .select .title{
  display: flex;
  align-items: center;
  height: 100%;
  margin-left: 14rpx;
  line-height: normal;
  color: #5a5a5a;
}
.wrap .editor-con .buy-wrap{
  display: flex;
  align-items: center;
}
.wrap .editor-con .buy-wrap .total{
  margin-right: 24rpx;
}
.wrap .editor-con .total text{}
.wrap .editor-con .settle-account{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 188rpx;
  height: 100%;
  color: #fff;
}
.wrap .editor-con .compile-wrap{
  display: flex;
  align-items: center;
}
.wrap .editor-con .compile-wrap view{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 148rpx;
  height: 76rpx;
  margin-right: 26rpx;
  line-height: normal;
  border: 1px solid #f2f2f2;
  border-radius: 50px 50px;
  overflow: hidden;
}
.wrap .editor-con .compile-wrap .collect{
  width: 180rpx;
}
.wrap .footer{
  width: 100%;
  height: 100rpx;
}