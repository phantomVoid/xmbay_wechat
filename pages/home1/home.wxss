.pop-swiper {
  height: 400rpx;
  margin-top: 28rpx;
}

.pop-swiper image {
  width: 100%;
  height: 350rpx;
  transform: scaleY(0.8);
  transition: all 0.3s ease;
  opacity: 0.8;
}

.pop-swiper .scale {
  transform: scaleY(1.0);
  transition: all 0.3s ease;
  opacity: 1.0;
}

.recommend-con {
  box-sizing: border-box;
}

.recommend-cnxh {
  padding: 0 15rpx;
}
