  border: none;
}

.back-top {
  position: fixed;
  right: 30rpx;
  bottom: -80rpx;
  width: 80rpx;
  height: 80rpx;
  z-index: 13;
}

/* scroll-view取消滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.board {
  position: fixed;
  bottom: -100%;
  left: 0;
  z-index: 11;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0);
}

.board .content {
  width: 90%;
  left: 10%;
  top: 0;
  height: 100%;
  background: white;
  position: absolute;
  overflow: hidden;
}

.filtrate-board {
  z-index: 14 !important;
  left: 100% !important;
  transition: all 0.5s ease;
}

.filtrate-board-anim {
  left: 0 !important;
  transition: all 0.5s ease;
}

.filtrate-item {
  padding: 50rpx 40rpx 0;
}

.filtrate-item .filtrate-title {
  font-size: 30rpx;
}

.filtrate-item .filtrate-content {
  overflow: hidden;
  width: 100%;
  margin-top: 16rpx;
}

.filtrate-item .filtrate-content > view {
  margin-top: 20rpx;
  width: 31%;
  margin-left: 3%;
  float: left;
  text-align: center;
  font-size: 26rpx;
  line-height: 70rpx;
  background: #f7f7f7;
  border-radius: 6rpx;
}

.filtrate-item .filtrate-content > view:nth-child(3n + 1) {
  margin-left: 0;
}

.filtrate-item .filtrate-price {
  width: 100%;
  overflow: hidden;
  display: flex;
  align-items: center;
  margin-top: 36rpx;
}

.filtrate-item .filtrate-price input {
  width: 260rpx;
  text-align: center;
  font-size: 26rpx;
  line-height: 70rpx;
  height: 70rpx;
  float: left;
  background: #f7f7f7;
  border-radius: 6rpx;
}

.filtrate-item .filtrate-price .line {
  width: 2%;
  margin: 0 4%;
  display: inline-block;
  height: 2rpx;
  background: #a9a9a9;
}

.board .content .footer {
  width: 100%;
  height: 90rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  overflow: hidden;
  border-top: 2rpx solid #eaeaea;
  z-index: 110;
  background: white;
}

.board .content .footer > view {
  width: 50%;
  float: left;
  text-align: center;
  font-size: 28rpx;
  line-height: 90rpx;
}

.board .content .footer > view:last-child {
  background: #f23030;
  color: white;
}

.block {
  width: 100%;
  height: 110rpx;
}

.classify-board {
  z-index: 12 !important;
  background: rgba(0, 0, 0, 0.3);
}

.classify-board > view {
  width: 100%;
  background: white;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.classify-board > view:first-child {
  display: flex;
  justify-content: space-between;
  margin-top: 180rpx;
}

.classify-board > view > .left {
  font-size: 28rpx;
  line-height: 90rpx;
  padding-left: 50rpx;
}

.classify-board > view .right {
  padding-right: 50rpx;
}

.classify-board > view .right>image {
  width: 28rpx;
  height: 20rpx;
}

.evaluate-detail {
  width: 92%;
  padding: 0 4%;
}

.evaluate-detail .detail-head {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.evaluate-detail .detail-head .head1 {
  display: flex;
  align-items: center;
}

.evaluate-detail .detail-head .avatar {
  width: 60rpx;
  height: 60rpx;
