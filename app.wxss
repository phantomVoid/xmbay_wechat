  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.evaluate-detail .pic-list {
  overflow: hidden;
  margin-top: 20rpx;
}

.evaluate-detail .pic-list > view {
  overflow: hidden;
  position: relative;
}

.evaluate-detail .pic-list > view .pic {
  width: 100%;
  height: 230rpx;
}

.evaluate-detail .pic-list > view .play-pic {
  width: 84rpx;
  height: 84rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -42rpx;
  margin-left: -42rpx;
}

.evaluate-detail .pic-list > view:nth-child(3n) {
  margin-right: 0;
}

.evaluate-detail .detail-time {
  margin-top: 26rpx;
  font-size: 26rpx;
  color: #bbb;
}

.evaluate-detail .detail-time text {
  margin-right: 30rpx;
}

