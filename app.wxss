.tab-more {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 10;
}

.tab-more .tab-more-head {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: white;
}

.tab-more .tab-more-head view {
  font-size: 30rpx;
  font-weight: bold;
  margin-left: 26rpx;
}

.tab-more .tab-more-head image {
  position: relative;
  width: 22rpx;
  height: 12rpx;
  padding: 24rpx;
}

.tab-more .tab-more-head image::after {
  content: '';
  position: absolute;
  left: 0;
  top: 50%;
  width: 2rpx;
  height: 34rpx;
  margin-top: -17rpx;
  background: #eee;
}

.tab-more .tab-more-wrap {
  overflow: hidden;
  padding-bottom: 34rpx;
  background: white;
}

.tab-more .tab-more-wrap > view {
  width: 154rpx;
  font-size: 26rpx;
  line-height: 48rpx;
  text-align: center;
  float: left;
  margin-top: 34rpx;
  margin-left: 24rpx;
  border-radius: 6rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  border: 1px solid #dbdbdb;
}

.empty-view {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100vh;
}

.empty-view > view {
  width: 100%;
}

.empty-view > view > image {
  width: 100%;
  height: 250rpx;
}

.empty-view > view > view {
  font-size: 32rpx;
  text-align: center;
}

.empty-view .no-order-head {
  font-size: 30rpx;
  text-align: center;
}

.empty-view .no-order-content {
  font-size: 26rpx;
  text-align: center;
  margin-top: 10rpx;
  color: #8d8d8d;
}

.empty-view > view .empty-tips {
  font-size: 26rpx;
  text-align: center;
  margin-top: 10rpx;
  color: #8d8d8d;
}

.copyright {
  text-align: center;
  padding: 10rpx 0 120rpx;
}

.copyright .company {
  font-size: 26rpx;
  color: #a2a7a8;
}

.copyright .credentials {
  font-size: 25rpx;
  padding-top: 15rpx;
  color: #6babe4;
}

.copyright image {
  width: 250rpx;
  height: 80rpx;
}

.iconPar {
  display: block;
  width: 60rpx;
  height: 60rpx;
  overflow: hidden;
  margin: 0 auto;
}

.iconColor {
  position: relative;
  transform: translateX(-60rpx);
  width: 60rpx;
  height: 60rpx;
  border-right: 60px solid transparent;
  display: block;
  filter: drop-shadow(60rpx 0 0 #888);
}
