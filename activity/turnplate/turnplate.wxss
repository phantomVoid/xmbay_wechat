/* activity/turnplate.wxss */

page {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.warp {
  position: relative;
  width: 100%;
  min-height: 100%;
  height: auto;
}

.regulation {
  position: absolute;
  right: 0;
  top: 20rpx;
  font-size: 28rpx;
  width: 155rpx;
  height: 52rpx;
}

.regulation view {
  height: 52rpx;
  line-height: 52rpx;
  margin-left: 25rpx;
  color: #fff;
  font-weight: bold;
  text-shadow: 0 0 10rpx #de5e2b;
}

.regulation image {
  position: absolute;
  right: 0;
  top: 0;
  width: 155rpx;
  height: 52rpx;
  z-index: 1;
}

.game-name {
  padding-top: 100rpx;
  padding-bottom: 50rpx;
  font-size: 70rpx;
  text-align: center;
  font-weight: bold;
  color: #fff;
}
.game-con{
  position: relative;
  display: flex;
}
.game-con .g-bg{
  width: 100%;
  display: block;
}
.game-con .light-con{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.game-con .light-con image{
  position: absolute;
  width:20rpx;
  height: 22rpx;
}
.game-con .box-con{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  padding: 80rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  flex-flow: wrap;
  z-index: 10;
}
.game-con .box-con .box_bg{
  width: 100%;
  display: block;
}
.game-con .box0{
  position: absolute;
  top: 14%;
  left: 14%;
  width: 22%;
}
.game-con .box1{
  position: absolute;
  top: 14%;
  left: 39%;
  width: 22%;
}
.game-con .box2{
  position: absolute;
  top: 14%;
  left:64%;
  width: 22%;
}
.game-con .box3{
  position: absolute;
  top: 38%;
  left: 64%;
  width: 22%;
}
.game-con .box4{
  position: absolute;
  top: 62%;
  left: 64%;
  width: 22%;
}
.game-con .box5{
  position: absolute;
  top: 62%;
  left: 39%;
  width: 22%;
}
.game-con .box6{
  position: absolute;
  top: 62%;
  left: 14%;
  width: 22%;
}
.game-con .box7{
  position: absolute;
  top: 38%;
  left: 14%;
  width: 22%;
}
.game-con .btn{
  position: absolute;
  top: 38%;
  left: 39%;
  width: 22%;
}
/* .game-con .box_bg{
  width: 100%;
} */
.game-con .prize-con{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  text-align: center;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.game-con .active{
  border: 8rpx solid #f25836;
}

.game-con .prize-con image{
  width: 80rpx;
  height: 80rpx;
  margin-top: 10rpx;
}
.game-con .prize-con view{
  font-size: 23rpx;
  line-height: 19rpx;
  color: #871f0b;
}
.game-con .prize-con .xxcy{
  margin-top: 55rpx;
  font-size: 26rpx;
  color: #ff5836;
  font-weight: bold;
}
.game-con .btn .btn-con{
  width: 100%;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
}
.game-con .btn .btn-con view{
  font-size: 30rpx;
  font-weight: bold;
  color: #eb3722;
  margin-top: 20rpx;
}
.game-con .btn .btn-con text{
  font-size: 26rpx;
  color: #c93f1e;
  margin-top: 20rpx;
  background-color: #f5ba40;
  border-radius: 50rpx;
  padding: 2rpx 15rpx;
}

.game_canvas {
  margin: 0 auto;
}

.win-list {
  position: relative;
  margin-top: 60rpx;
  padding: 0 30rpx 20rpx;
  width: 100%;
  box-sizing: border-box;
}

.win-list .bg-img {
  width: 100%;
  height: 500rpx;
}

.win-list .list-con {
  position: absolute;
  top: 120rpx;
  left: 0;
  width: 100%;
  height: 350rpx;
  padding: 0 80rpx 20rpx;
  box-sizing: border-box;
}

.win-list .list-con .row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  padding: 20rpx 0;
}

.win-list .list-con .row .user_name {
  width: 150rpx;
  color: #fc4a36;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.win-list .list-con .row .phone {
  color: #8b8548;
  flex: 1;
  padding-left: 40rpx;
}

.win-list .list-con .row .win {
  width: 200rpx;
  color: #8b8548;
  text-align: right;
}

.win_goods{
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 50;
  display: flex;
  justify-content: center;
  align-items: center;
}
.win_goods .win_warp{
  position: relative;
  width: 70%;
  height: 580rpx;
  border-radius: 10rpx;
}
.win_goods .win_con{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  border-radius: 10rpx;
  z-index: 52;
  /* display: flex;
  align-items: center;
  flex-direction: column; */
  padding: 30rpx;
  box-sizing: border-box;
}
.win_goods .win_con .jg{
  font-size: 28rpx;
  color: #3c3c3c;
  margin-top: 30rpx;
  text-align: center;
}
.win_goods .win_warp .bg_ray{
  position: absolute;
  left: 50%;
  top: -40%;
  z-index: 51;
  width: 690rpx;
  height: 690rpx;
  margin-left: -345rpx;
}
.win_con .goods_img{
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto;
}
.win_con text{
  margin-top: 40rpx;
  font-size: 26rpx;
  color: #3c3c3c;
}

.win_info{
  width: 90%;
  margin: 0 auto;
}
.win_info .win_infowarp .tiit{
  margin-top: 15rpx;
  font-size: 24rpx;
  color: #666666;
}
.center{
  text-align: center;
}
.win_info .win_infowarp .ems_info{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.win_info .win_infowarp .ems_info .info{
  font-size: 24rpx;
  color: #666666;
  line-height: 40rpx;
}
.win_info .win_infowarp .ems_info image{
  width: 27rpx;
  height: 26rpx;
}
.win_info .win_infowarp .add{
  font-size: 24rpx;
  color: #666666;
  text-align: left;
}
.wingoods_btn{
  display: flex;
  justify-content: space-between;
}
.winjf_btn{
  display: flex;
  justify-content: center;
}
.wingoods_btn text{
  border-radius: 50px;
  border: 1rpx solid transparent;
  padding: 10rpx 35rpx
}
.wingoods_btn text:last-child{
  /* background-color: #f23030; */
  color: #fff;
  /* padding: 10rpx 65rpx */
}
.close{
  position: absolute;
  bottom: -20%;
  left: 50%;
  z-index: 51;
  width: 59rpx;
  height: 59rpx;
  margin-left: -30rpx;
}
.no-order {
  height: calc(100vh - 170rpx);
  display: flex;
  align-items: center;
  justify-content: center;
}

.no-order > view {
  width: 100%;
}

.no-order image {
  width: 100%;
  height: 260rpx;
}

.no-order .no-order-head {
  font-size: 30rpx;
  text-align: center;
}

.no-order .no-order-content {
  font-size: 26rpx;
  text-align: center;
  margin-top: 10rpx;
  color: #8d8d8d;
}
