.collect-head {
  height: 90rpx;
  font-size: 30rpx;
  overflow: hidden;
  background: white;
}

.collect-head > view {
  width: 25%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  float: left;
}

.collect-head > view image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 10rpx;
}

.list {
  margin-top: 10rpx;
  background: white;
}

.list > view {
  position: relative;
  display: flex;
  align-items: center;
  height: 156rpx;
  padding:0 24rpx;
}

.list .shop-img {
  width: 98rpx;
  height: 98rpx;
  filter: grayscale(80%);
}

.list .next {
  position: absolute;
  right: 28rpx;
  top: 50%;
  width: 12rpx;
  height: 22rpx;
  margin-top: -11rpx;
}

.list > view > .shop-con {
  margin-left: 24rpx;
}

.list .shop-name {
  font-size: 28rpx;
  line-height: 28rpx;
}

.list .shop-attention{
  font-size: 26rpx;
  margin-top: 10rpx;
  color: #8d8d8d;
}

.list .invalid{
  display: flex;
  align-items: center;
  padding: 10rpx 18rpx;
  border-radius: 50px 50px;
  overflow: hidden;
  margin-left: auto;
  background-color: #e3e3e3;
}
.list .invalid .icon{
  width: 23rpx;
  height: 23rpx;
}
.list .invalid text{
  margin-left: 10rpx;
  font-size: 22rpx;
  color: #999;
}
.invalid_type{
  background-color: #f6f6f6;
}
.invalid_type .shop-name {
  color: #999fa5;
}

.invalid_type .shop-attention{
  color: #999fa5;
}
