}
.map{
  width: 100%;
  height: 70vh;
}
.warp{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 29%;
  background: #fff;
}
.warp .top{
  width: 100%;
  padding-top: 20rpx;
}
.warp .top>view{
  width: 52rpx;
  height: 2rpx;
  background-color: #e5e5e5;
  margin: 10rpx auto;
}
.warp .tip{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 35rpx 0;
}
.warp .tip text{
  font-size: 36rpx;
  color: #17191b;
}
.warp .tip image{
  width: 11rpx;
  height: 21rpx;
  margin-left: 10rpx;
}
.warp .yuji{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0;
}
.warp .yuji image{
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.warp .yuji view{
  font-size: 25rpx;
  color: #616161;
}
.warp .yuji view>text{
  font-size: 25rpx;
  color: #297bcb;
}

.warp .qs_con{
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.warp .qishou{
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  box-sizing: border-box;
  border-top: 1px dashed #d9d9d9;
}
.warp .qishou .qs_img{
  width: 90rpx;
  height: 90rpx;
  margin-right: 20rpx;
}
.warp .qishou .qs_info{
  display: flex;
  align-items: center;
  flex: 1;
}
.warp .qishou .qs_info .name{
  font-size: 28rpx;
}
.warp .qishou .qs_info .icon{
  position: relative;
  margin-left: 20rpx;
}
.warp .qishou .qs_info .icon image{
  /* position: absolute;
  top: 0;
  left: 0; */
  width: 104rpx;
  height: 28rpx;
  display: block;
}
.warp .qishou .qs_info .icon text{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
}
.warp .qishou .phone{
  width: 35rpx;
  height: 35rpx;
}

.info_con{
  position: fixed;
  left: 0;
  bottom: -100vh;
  z-index: 20;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}
.mu{
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  transition: 1s all;
}
.mu-on{
  background-color: rgba(0, 0, 0, 0.2);
}

.info_con .list_con{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 70%;
  z-index: 21;
  background-color: #fff;
}
.info_con .show{
  bottom: 0;
}
.info_con .list_con .title{
  font-size: 29rpx;
  padding: 35rpx 0;
  border-bottom: 1px solid #f6f6f6;
  text-align: center;
}
.info_con .list_con .list{
  padding: 30rpx 60rpx;
}
.info_con .list_con .list .li{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 36rpx 0;
}
.info_con .list_con .list .li .li_icon{
  width: 40rpx;
  display: flex;
  justify-content: center;
  margin-right: 20rpx;
}
.info_con .list_con .list .li .icon{
  width: 16rpx;
  height: 16rpx;
  background-color: #616161;
  border-radius: 50%;
}
.info_con .list_con .list .li .on-icon{
  width: 12rpx;
  height: 12rpx;
  background-color: #fff;
  border-radius: 50%;
  border:6rpx solid transparent;
}
.info_con .list_con .list .li cover-image{
  width: 22rpx;
  height: 22rpx;
}
.info_con .list_con .list .li .tit{
  font-size: 25rpx;
  flex: 1;
}
.info_con .list_con .list .li .time{
  font-size: 20rpx;
  color: #8c8c8c;
}
.info_con .list_con .sub{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #f23030;
  padding: 32rpx 0;
  font-size: 30rpx;
  text-align: center;
  color: #fff;
}