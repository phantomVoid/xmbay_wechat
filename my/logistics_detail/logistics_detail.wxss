.logistics-info:last-child .address-info::after {
  width: 1rpx;
  height: 50%;
  position: absolute;
  bottom: 0;
  content: '';
  left: -1rpx;
  background: white;
}

.address-info .circle {
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: #cdcdcd;
  position: absolute;
  left: 0;
  top: 8rpx;
  margin-top: -8rpx;
  margin-left: -8rpx;
  z-index: 3;
}

.address-info .detail {
  margin-left: 34rpx;
  padding: 20rpx 0;
  min-height: 100rpx;
}

.address-info .detail>view {
  transform: translate(0, -34rpx);
}
.address .address-info .detail>view {
  transform: translate(0, -14rpx);
  color: #808080;
}

.empty-view{
  height:50vh;
  padding-bottom:170rpx;
}