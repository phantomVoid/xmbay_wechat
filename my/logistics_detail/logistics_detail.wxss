.logistics-con {
  display: flex;
  flex-direction: column;
}

.good-info {
  width: 100%;
  padding: 18rpx 60rpx 40rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  box-sizing: border-box;
}

.good-info > image {
  width: 172rpx;
  height: 172rpx;
  flex-shrink: 0;
}

.good-info > view {
  width: 450rpx;
  margin-left: 18rpx;
}
.good-info > view .price{
  display: flex;
  align-items: baseline;
}

.good-info > view .good-name {
  font-size: 28rpx;
  height: 76rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.good-info > view .price .single_price{
  font-size: 30rpx;
}
.good-info > view .price .single_title{
  font-size: 28rpx;
  color: #8d8d8d;
}

.signed {
  width: 710rpx;
  height: 144rpx;
  margin: 0 auto;
  color: white;
  /* background: linear-gradient(to right, #fe552b, #f20230); */
  border-radius: 10rpx;
  transform: translateY(-20rpx);
}

.signed .signed-title {
  font-size: 30rpx;
  margin-left: 36rpx;
  padding-top: 30rpx;
}

.signed .signed-content {
  font-size: 28rpx;
  margin-left: 36rpx;
  margin-top: 6rpx;
}

.logistics-star {
  width: 710rpx;
  padding: 30rpx 0;
  padding-bottom: 0;
  margin: 0 auto 20rpx;
  background: white;
  border-radius: 10rpx;
  box-shadow: 0 0 10rpx 0 rgba(0, 0, 0, 0.1);
}
.logistics-star .address{
  display: flex;
}
.logistics-star .address .icon{
  width: 114rpx;
  flex-shrink: 0;
}
.logistics-star .address .address-info text{
  position: absolute;
  left: -25rpx;
  top: 0;
  width: 51rpx;
  height: 51rpx;
  border-radius: 50%;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  line-height: 51rpx;
  display: block;
}
.logistics-star .address .add-info{
  font-size: 26rpx;
  color: #868686;
  margin-left:34rpx;
  flex: 1;
}

.logistics-star .logistics-company {
  font-size: 26rpx;
  color: #bbb;
  margin-left: 30rpx;
}

.logistics-star .star-level {
  display: flex;
  align-items: center;
  margin: 40rpx 0 40rpx 30rpx;
}

.logistics-star .star-level > image {
  width: 90rpx;
  height: 90rpx;
}

.logistics-star .star-level > view {
  display: flex;
  align-items: center;
  margin-left: 44rpx;
}

.logistics-star .star-level > view image {
  width: 54rpx;
  height: 54rpx;
  margin-right: 28rpx;
}

.logistics-star .express-num {
  font-size: 26rpx;
  line-height: 62rpx;
  padding-left: 34rpx;
  background: #fafafa;
}

.logistics-info {
  color: #b8b8b8;
  display: flex;
  align-items: center;
}

.logistics-info .date-left {
  float: left;
  width: 114rpx;
  padding: 20rpx 0;
  transform: translate(0, -50rpx);
}

.date-day {
  font-size: 24rpx;
  text-align: center;
}

.date-time {
  font-size: 20rpx;
  text-align: center;
}

.address-info {
  position: relative;
  float: left;
  width: 550rpx;
  font-size: 28rpx;
  border-left: 2rpx solid #dadada;
}


.logistics-info:first-child{
  color: #333;
}

.address .address-info {
  border-left: 2rpx dashed #b8b8b8;
}

.logistics-info:last-child .address-info {
  border: none;
}

.logistics-info:first-child .date-day {
  font-size: 28rpx;
}

