.head {
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
  position: relative;
  background: white;
}

.head .head-img {
  width: 114rpx;
  height: 114rpx;
  border-radius: 50%;
  margin-left: 24rpx;
}

.head > view {
  width: 60%;
  margin-left: 30rpx;
}

.head .nickname {
  font-size: 30rpx;
  line-height: 30rpx;
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.head .username {
  font-size: 26rpx;
  line-height: 26rpx;
  margin-top: 18rpx;
  color: #8d8d8d;
}

.next {
  position: absolute;
  right: 26rpx;
  top: 50%;
  margin-top: -11rpx;
  width: 12rpx;
  height: 22rpx;
}

.wrap {
  padding-left: 24rpx;
  margin-top: 16rpx;
  background: white;
}
.exit {
  text-align: center;
  margin-top: 16rpx;
  background: white;
}

.content {
  font-size: 30rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #f6f6f6;
  position: relative;
}
