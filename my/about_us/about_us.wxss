.logo {
  width: 190rpx;
  height: 190rpx;
  display: flex;
  margin: 100rpx auto 40rpx;
  border-radius: 30rpx;
  overflow: hidden;
  background-color: #fff;
}

.version {
  font-size: 28rpx;
  text-align: center;
  margin-bottom: 90rpx;
}

.content {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: white;
  border-bottom: 1px solid #f6f6f6;
}

.content view {
  font-size: 30rpx;
  margin-left: 30rpx;
}

.content image {
  width: 12rpx;
  height: 22rpx;
  margin-right: 30rpx;
}
