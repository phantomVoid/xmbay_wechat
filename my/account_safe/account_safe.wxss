.wrap {
  padding-left: 26rpx;
  background: white;
}
.icon-wd-zhyaq-wx{
  font-size: 40rpx;
  color: #f7a22e;
}
.icon-wd-zhyaq-anq{
  font-size: 40rpx;
  color: #32cb33;
}
.content {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #f6f6f6;
}

.content .left {
  display: flex;
  align-items: center;
}

.content .left image {
  width: 36rpx;
  height: 36rpx;
}

.content .left .title {
  font-size: 30rpx;
  margin-left: 26rpx;
}

.content .right {
  display: flex;
  align-items: center;
}

.content .right image {
  width: 12rpx;
  height: 22rpx;
  margin-right: 26rpx;
}

.content .right view{
  font-size: 28rpx;
  margin-right: 20rpx;
  color: #8d8d8d;
}
