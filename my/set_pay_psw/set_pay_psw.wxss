page {
  background-color: white;
}

.wrap {
  width: 100%;
  padding: 50rpx 30rpx;
  box-sizing: border-box;
}

.wrap .title {
  font-size: 28rpx;
  line-height: 50rpx;
  margin-bottom: 20rpx;
  color: #333;
}

.wrap .psw-con {
  display: flex;
  align-items: center;
  width: 100%;
  height: 100rpx;
  border-radius: 10rpx;
  overflow: hidden;
  border: 1px solid #f1f1f1;
}

.wrap .psw-con .con {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  height: 100%;
}
.wrap .psw-con>.con::before {
  position: absolute;
  top: 0;
  right: 0;
  content: '';
  flex: 1;
  width: 1px;
  height: 100%;
  border-right: 1px solid #f6f6f6;
}
.wrap .psw-con .con:last-child::before {
  position: absolute;
  top: 0;
  right: 0;
  content: '';
  flex: 1;
  width: 1px;
  height: 100%;
  border-right: 1px solid transparent;
}
.wrap .psw-con .con view{
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  background-color: #555;
}

.wrap .psw {
  position: fixed;
  left: -100vw;
  top: -100vh;
}
