.no-order {
  margin-top: 170rpx;
  height: calc(100vh - 170rpx);
  display: flex;
  align-items: center;
  justify-content: center;
}

.no-order > view {
  width: 100%;
}

.no-order image {
  width: 100%;
  height: 260rpx;
}

.no-order .no-order-head {
  font-size: 32rpx;
  text-align: center;
}

.no-order .no-order-content {
  font-size: 26rpx;
  color: #8d8d8d;
  text-align: center;
  margin-top: 10rpx;
}

.distribution {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 84rpx;
  z-index: 2;
  background-color: rgba(0, 0, 0, 0.7);
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 18rpx 22rpx;
  box-sizing: border-box;
}

.distribution .left {
  display: flex;
  align-items: center;
}

.distribution .left .user_img {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
  background-color: #fff;
}

.distribution .left .distribution_text {
  width: 440rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 26rpx;
  color: #fbdc5d;
  margin-left: 15rpx;
}

.distribution .right {
  display: flex;
}

.distribution .right .distribution_btn {
  font-size: 22rpx;
  color: #5e3c0c;
  background-color: #fde66e;
  border-radius: 50px 50px;
  padding: 10rpx 16rpx;
}

.dyx {
  display: flex;
  align-items: center;
  flex-direction: column;
  width: 100vw;
  height: 100vh;
}

.dy-icon {
  flex-shrink: 0;
  width: 100%;
  height: 220rpx;
}

.dy-icon-bottom {
  flex-shrink: 0;
  width: 100%;
  height: 37rpx;
}

.content {
  flex: 1;
  width: 100%;
  height: 1%;
  padding: 4rpx 35rpx;
  box-sizing: border-box;
}

.content .good-scroll {
  height: 100%;
}

.content .good-scroll .goods-con {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.content .good-scroll .goods-con .item {
  position: relative;
  display: flex;
  width: 100%;
  height: 230rpx;
  padding: 25rpx 0;
  border-bottom: 1px solid #eee;
}

.content .goods-con .item .goods-file {
  flex-shrink: 0;
  width: 200rpx;
  height: 200rpx;
}

.content .goods-con .item .goods-file image {
  width: 100%;
  height: 100%;
}

.content .goods-con .item .goods-info {
  flex: 1;
  padding: 25rpx 24rpx;
  box-sizing: border-box;
}

.content .goods-con .item .goods-info .title {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  height: 88rpx;
  font-size: 26rpx;
  line-height: 40rpx;
  color: #333;
  overflow: hidden;
}

.content .goods-con .item .goods-info .tip {
}

.content .goods-con .item .goods-info .tip text {
  font-size: 20rpx;
  line-height: 50rpx;
  padding: 4rpx 6rpx;
  border-radius: 8rpx;
  border: 1px solid #ee5b0b;
  color: #ee5b0b;
}

.content .goods-con .item .goods-info .goods-price {
}

.content .goods-con .item .goods-info .goods-price text {
  font-size: 22rpx;
  color: #ee5b0b;
}

.content .goods-con .item .goods-info .goods-price .prcie {
  font-size: 30rpx;
}
.content .goods-con .item .goods-info .goods-btn {
  position: absolute;
  right: 20rpx;
  bottom: 38rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 130rpx;
  height: 50rpx;
  font-size: 22rpx;
  border-radius: 8rpx;
  color: #fff;
  background: linear-gradient(to bottom ,#ee5b0b,#ff8949);
  overflow: hidden;
}
