.head-wrap {
  width: 100%;
  height: 126rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.head {
  width: 680rpx;
  height: 66rpx;
  overflow: hidden;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
}

.head > view {
  width: 33.3%;
  font-size: 30rpx;
  line-height: 66rpx;
  text-align: center;
  border-right: 2rpx solid #f23030;
}

.head > view:last-child {
  border-right: none;
}

.head .cur-head {
  color: white;
}

.list {
  padding: 0 30rpx;
  background: white;
}

.list > view {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 2rpx solid #f5f5f5;
  padding: 24rpx 0;
}

.list .title {
  font-size: 30rpx;
  line-height: 30rpx;
}

.list .time {
  font-size: 24rpx;
  line-height: 24rpx;
  margin-top: 20rpx;
  color: #8d8d8d;
}

.list .money {
  display: flex;
  align-items: center;
}

.list .money image {
  width: 22rpx;
  height: 22rpx;
}

.list .money text {
  font-size: 30rpx;
  margin-left: 10rpx;
}

.empty-view{
  height: calc(100vh - 126rpx);
}