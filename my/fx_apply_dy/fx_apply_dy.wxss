/* my/fx_apply_dy/fx_apply_dy.wxss */
page{
  width: 100%;
  height: 100%;
}
.warp{
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 100%;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.warp .list{
  width: 100%;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  padding: 20rpx 34rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom: 1px solid #f6f6f6;
}
.warp picker{
  width: 100%;
}
.warp .list .warp_title{
  width: 140rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.warp .list input{
  flex: 1;
}
.warp .list .warp_sex{
  margin-right: auto;
  color: #959ba3;
}
.warp .list .more{
  width: 12rpx;
  height: 22rpx;
}

.submit{
  position: absolute;
  bottom: 30rpx;
  left:50%;
  width: 90%;
  border-radius: 10rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 28rpx;
  padding: 25rpx 0;
  margin-left: -45%;
  background-color: #ddd;
  color: #fff;
}