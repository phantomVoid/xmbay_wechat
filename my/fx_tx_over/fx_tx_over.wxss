page{
  background-color: #fff;
}
.container{
  position: relative;
}
.container .tx_con{
  position: relative;
  height: 330rpx;
}
.container .tx_con .tx-bj{
  position: absolute;
  left: 0;
  bottom: -30rpx;
  width: 100%;
  height: 79rpx;
}
.container .tx_con .tx_warp{
  position: absoluteb;
  left: 0;
  top: 0;
  width: 100%;
  height: 80%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
}
.container .tx_con .tx_warp .title{
  font-size: 50rpx;
  font-weight: bold;
  padding-bottom: 29rpx;
}
.container .tx_con .tx_warp .tip{
  font-size: 27rpx;
}
.container .icon{
  position: absolute;
  left: 50%;
  bottom: -30rpx;
  width: 135rpx;
  height: 135rpx;
  margin-left: -67.5rpx;
  border-radius: 50%;
  border: 8rpx solid #f23030;
}
.container .submit{
  width: 80%;
  padding: 25rpx 0;
  margin: 200rpx auto 0;
  color: #fff;
  background-color: #f23030;
  border-radius: 10rpx;
  text-align: center;
  font-size: 30rpx;
}