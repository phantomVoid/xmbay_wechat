.head {
  padding: 30rpx;
  background: #f6f6f6;
}

.head-title {
  font-size: 30rpx;
  line-height: 1;
  color: #acacac;
}

.head-content {
  margin-top: 40rpx;
  overflow: hidden;
}

.head-content > view {
  width: 196rpx;
  height: 54rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #f6f6f6;
  border: 2rpx solid #c9c9c9;
  border-radius: 6rpx;
  float: left;
  margin-right: 30rpx;
}

.head-content > view image {
  width: 28rpx;
  height: 20rpx;
  margin-right: 14rpx;
}

.head-content > view view {
  font-size: 28rpx;
}

.feedback {
  padding: 30rpx;
  width: 690rpx;
  height: 330rpx;
  font-size: 28rpx;
  background: white;
}

.upload-wrap {
  padding-left: 24rpx;
  padding-bottom: 40rpx;
}

.upload-wrap .upload-head {
  font-size: 28rpx;
  line-height: 86rpx;
  color: #8e8e8e;
}

.upload-wrap .pic-list {
  overflow: hidden;
}

.upload-wrap .pic-list > view {
  width: 168rpx;
  height: 168rpx;
  margin-right: 28rpx;
  position: relative;
  float: left;
  padding-right: 20rpx;
  padding-top: 20rpx;
}

.upload-wrap .pic-list .pic {
  width: 100%;
  height: 100%;
  background: white;
}

.upload-wrap .pic-list .delete {
  width: 42rpx;
  height: 42rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.concact {
  padding: 0 30rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.concact > view {
  width: 150rpx;
  font-size: 30rpx;
  line-height: 100rpx;
}

.concact input {
  font-size: 30rpx;
  line-height: 100rpx;
  height: 100rpx;
  text-align: right;
}

.submit {
  width: 692rpx;
  font-size: 32rpx;
  text-align: center;
  color: white;
  line-height: 84rpx;
  border-radius: 10rpx;
  margin: 86rpx auto 0;
}
