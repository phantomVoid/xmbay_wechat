/* my/service/service.wxss */
@import "popups/popups.wxss";
page {
  width: 100%;
  height: 100%;
  background-color: #f6f6f6;
}

.warp {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.service_header {
  display: flex;
  height: 82rpx;
  padding: 25rpx;
  box-sizing: border-box;
}

.service_header .service_btn {
  height: 100%;
  display: inline-flex;
  align-items: center;
  margin-left: auto;
}

.service_header .service_btn .go_shop {
  font-size: 24rpx;
  border-radius: 50px 50px;
  padding: 7rpx 25rpx;
  color: #fff;
  border: 0.5px solid #fff;
}

.service_header .service_btn .service_set {
  width: 38rpx;
  height: 36rpx;
  padding: 0 20rpx 0 34rpx;
  margin-left: 10rpx;
}

.service_info {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  padding-bottom: 20rpx;
}

.service_info .goods {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 25rpx 20rpx 10rpx;
  box-sizing: border-box;
}

.service_info .goods .goods_img {
  width: 127rpx;
  height: 127rpx;
  display: block;
  flex-shrink: 0;
  padding-right: 32rpx;
}

.service_info .goods .goods_info {
  width: 76%;
}

.service_info .goods .goods_info .goods_title {
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.service_info .service_info_btn {
  display: inline-block;
  font-size: 28rpx;
  border-radius: 50px 50px;
  padding: 12rpx 24rpx;
  border: 1px solid #f22b2a;
}

.goods_price .unit {
  font-size: 28rpx;
}

.goods_price .integer {
  font-size: 36rpx;
}

.goods_price .decimal {
  font-size: 28rpx;
}

.message_con {
  position: relative;
  height: auto;
  flex: 1;
}

.message_con scroll-view {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.message_con scroll-view .msg {
  height: auto;
  padding-bottom: 40rpx;
}

.message_con scroll-view .msg .msgtime {
  width: 100%;
  text-align: center;
  padding: 20rpx 0;
  margin-bottom: 20rpx;
}

.message_con scroll-view .msg .msgtime text {
  padding: 14rpx;
  font-size: 22rpx;
  background-color: #dbdcde;
  color: #fff;
}

.message_con scroll-view .msg .msginfo {
  height: auto;
  display: flex;
}

.message_con scroll-view .msg .userimg {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  overflow: hidden;
  display: block;
  background-color: #fff;
}

.message_con scroll-view .other .msg-con::before {
  position: absolute;
  top: 30rpx;
  left: -6px;
  content: '';
  width: 0;
  height: 0;
  border-top: 6px solid transparent;
  border-bottom: 6px solid transparent;
  border-right: 6px solid #fff;
}

.message_con scroll-view .msg .msg-con {
  position: relative;
  max-width: 65%;
  padding: 20rpx 30rpx;
  margin-left: 30rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
  word-break: break-all;
  background-color: #fff;
}

.message_con scroll-view .msg .msg-con .msg_image {
  width: 10rpx;
  height: 10rpx;
  max-width: 400rpx;
}

.message_con scroll-view .me .msginfo {
  flex-direction: row-reverse;
}

.message_con scroll-view .me .msg-con::before {
  position: absolute;
  top: 30rpx;
  right: -6px;
  content: '';
  width: 0;
  height: 0;
  border-top: 6px solid transparent;
  border-bottom: 6px solid transparent;
  border-left: 6px solid #fff;
}

.message_con scroll-view .me .msg-con {
  position: relative;
  max-width: 65%;
  padding: 20rpx 30rpx;
  margin-right: 30rpx;
  font-size: 28rpx;
  border-radius: 10rpx;
  word-break: break-all;
  background-color: #fff;
}

.message_con scroll-view .msg .msg-con .msg_text {
  display: flex;
  align-items: center;
  flex-direction: row;
  flex-wrap: wrap;
}

.message_con scroll-view .msg .msg-con .msg_text image {
  width: 60rpx;
  height: 60rpx;
}

.message_con scroll-view .me .msg-con .msg_voice {
  display: flex;
  align-items: center;
}

.message_con scroll-view .me .msg-con .msg_voice .voice_image {
  width: 29rpx;
  height: 32rpx;
  transform: rotate(180deg);
  padding: 0 20rpx;
}

.message_con scroll-view .me .msg-con .msg_voice .voice_time {
  font-size: 24rpx;
}

.message_con scroll-view .me .msg-con .msg_goods {}

.message_con scroll-view .me .msg-con .msg_goods {}

.message_con scroll-view .msg .msg-con .msg_goods .goods {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}

.message_con scroll-view .msg .msg-con .msg_goods .goods .goods_img {
  width: 127rpx;
  height: 127rpx;
  display: block;
  flex-shrink: 0;
  margin-right: 32rpx;
  background-color: #f6f6f6;
}

.message_con scroll-view .msg .msg-con .msg_goods .goods .goods_info {
  width: 76%;
}

.message_con scroll-view .msg .msg-con .msg_goods .goods .goods_info .goods_title {
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.on-title{
  display: inline-block;
  width: 240rpx;
  height: 60rpx;
  background-color: #f6f6f6;
}
.on-price{
  display: inline-block;
  width: 140rpx;
  height: 30rpx;
  margin-top: 20rpx;
  background-color: #f6f6f6;
}
/*  */

.message_con scroll-view .me .msg-con .type {
  position: absolute;
  left: -50rpx;
  top: 35%;
  width: 15px;
  height: 15px;
}

@keyframes rotate {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(720deg);
  }
}

.message_con scroll-view .me .msg-con .rotate {
  animation: rotate 2s;
  animation-iteration-count: infinite;
}

.service_footer {
  width: 100%;
  box-sizing: border-box;
  background-color: #f2f2f2;
}

.service_footer .service_con {
  position: relative;
  display: flex;
  align-items: center;
  padding: 20rpx 24rpx;
  background-color: #f2f2f2;
}

.service_footer .service_type {
  width: 53rpx;
  height: 53rpx;
  display: block;
  border-radius: 50%;
  margin-right: 20rpx;
}

.service_footer .service {
  flex: 1;
  border-radius: 50rpx 50rpx;
  margin-right: 20rpx;
  padding: 10rpx 30rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
}

.service_footer .service textarea {
  width: 88%;
  /* height: 40rpx; */
  min-height: 40rpx;
  height: auto;
  padding: 10rpx 0;
  /* box-sizing: border-box; */
  font-size: 32rpx;
  /* flex: 1; */
}

.service_footer .service_bq {
  position: absolute;
  right: 16%;
  top: 50%;
  margin-top: -26.5rpx;
  width: 53rpx;
  height: 53rpx;
  display: block;
  margin-left: 20rpx;
  border-radius: 50%;
}

.service_footer .service>.speak {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  padding: 8rpx 0;
}

.service_footer .service_fun {
  width: 53rpx;
  height: 53rpx;
  border-radius: 50%;
  display: block;
  background: linear-gradient(to right, #fe5227, #f22b2a);
}

.service_footer .submit {
  width: 53rpx;
  height: 53rpx;
  font-size: 26rpx;
  line-height: 53rpx;
}

.funbtn_con {
  height: 380rpx;
  display: flex;
  padding: 46rpx;
  background-color: #f3f3f3;
}

.funbtn_con .btn_con {
  height: 180rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin-right: 46rpx;
}

.funbtn_con .btn_con>image {
  width: 121rpx;
  height: 121rpx;
}

.funbtn_con .btn_con>text {
  font-size: 24rpx;
  color: #555;
  margin-top: 14rpx;
}

.emoji_con {
  height: 472rpx;
  /* padding: 46rpx; */
  background-color: #f3f3f3;
}

.emoji_con swiper {
  width: 100%;
  height: 100%;
  /* display: flex; */
}

.emoji_con .swiper-item {
  /* display: flex; */
}

.emoji_con .emoji {
  width: 120rpx;
  height: 120rpx;
  display: inline-flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
}

.emoji_con .emoji .emoji_img {
  width: 60rpx;
  height: 60rpx;
}

.emoji_con .emoji .emoji_del {
  width: 60rpx;
  height: 44rpx;
  padding-top: 16rpx;
  transform: translateY(-10rpx);
}

.emoji_con .emoji .emoji_name {
  font-size: 24rpx;
}

.touchVoice {
  position: fixed;
  top: 40%;
  left: 50%;
  margin-left: -205rpx;
}

.touchVoice .record {
  width: 310rpx;
  height: 300rpx;
  background-color: rgba(0, 0, 0, 0.6);
  border-radius: 10rpx;
  padding: 50rpx;
}

.touchVoice .record .chat_icon2 {
  width: 115rpx;
  height: 189rpx;
  margin: 0 50rpx 0 30rpx;
}

.touchVoice .record .chat_icon1 {
  width: 73rpx;
  height: 139rpx;
}

.touchVoice .record .record_text {
  font-size: 30rpx;
  color: #fff;
  margin-top: 40rpx;
  text-align: center;
}

.touchVoice .record_stop {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.touchVoice .record_stop .chat_icon3 {
  width: 148rpx;
  height: 191rpx;
  text-align: center;
}

.touchVoice .record_stop .record_text_s {
  font-size: 30rpx;
  color: #fff;
  background-color: rgba(216, 61, 61, 0.9);
  border-radius: 10rpx;
  margin-top: 20rpx;
  padding: 20rpx;
}
