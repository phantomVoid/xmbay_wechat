.wrap {
  padding: 0 30rpx;
}

.member-wrap {
  position: relative;
  /* height: 580rpx; */
  height: 600rpx;
  margin-top: 28rpx;
}

.member-wrap > image {
  width: 100%;
  height: 100%;
}

.member-wrap > view {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.member-head {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 35rpx;
  margin-bottom: 12rpx;
}

.member-head > image {
  width: 118rpx;
  height: 118rpx;
  border-radius: 50%;
  box-sizing: border-box;
  border: 4rpx solid white;
  box-shadow: 0 1rpx 15rpx 0 rgba(158, 113, 15, 0.3);
}

.member-info {
  text-align: center;
}

.member-info .member-level {
  position: relative;
  width: 100%;
}

.member-info .member-level>text {
  font-size: 24rpx;
  padding: 6rpx 25rpx;
  border-radius: 50rpx 50rpx;
  color: #e5cda3;
  background: linear-gradient(to right, #9c7a2a, #84661f);
  box-shadow: 0 2rpx 16rpx 0 rgba(158, 113, 15, 0.3);
}

.member-info .member-level .next-level {
  position: absolute;
  right: 0;
  top: 0;
  z-index: -1;
  width: 50%;
  height: 9rpx;
  transform: translate(-8rpx, 25rpx);
  background-color: #b58f3e;
}

.member-info .member-level .next-level>view {
  width: 52rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  font-size: 24rpx;
  transform: translate(270rpx, -20rpx);
  border-radius: 50%;
  color: #b58f3e;
  background-color: #dbbe89;
  box-shadow: 0 0 15rpx 0 rgba(158, 113, 15, 0.3);
}

.member-info .member-levelNum {
  width: 100%;
  font-size: 24rpx;
  padding: 10rpx 0;
  color: #7d5f16;
}

.member-info .member-tip {
  width: 100%;
  font-size: 22rpx;
  color: #a57f2f;
}

.member-head > view {
  margin-left: 30rpx;
}

.member-head .member-name {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #866823;
}

.member-head .member-grade {
  width: 40rpx;
  font-size: 24rpx;
  line-height: 32rpx;
  text-align: center;
  margin-top: 18rpx;
  color: #eee6c9;
  background: #84661f;
}

.privilege-head {
  text-align: center;
  font-size: 30rpx;
  margin-top: 35rpx;
}

.privilege-content {
  display: flex;
  align-items: center;
  overflow: hidden;
  height: 180rpx;
  margin-top: 20rpx;
}

.privilege-content .privilege {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  width: 65%;
  height: 100%;
  padding: 30rpx 0 20rpx 40rpx;
  box-sizing: border-box;
}
.privilege-content .privilege-icon{
  flex: 1;
  display: flex;
  justify-content: center;
  height: 113rpx;
}
.privilege-content .privilege-icon image {
  width: 158rpx;
  height: 113rpx;
}

.privilege-content .privilege .title {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
}

.privilege-content .privilege .tips {
  font-size: 26rpx;
  color: #a57f2f;
}

.privilege-content .privilege .tips .ps{
  padding-left: 10rpx;
  color: #dfc390;
}

.rule-img {
  width: 100%;
  height: 35rpx;
  display: block;
  margin: 20rpx 0 40rpx;
}

.level-one {
  position: relative;
  width: 674rpx;
  height: 170rpx;
  margin: 0 auto 10rpx;
}

.level-one > image {
  width: 100%;
  height: 100%;
}

.level-one > view {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  display: flex;
  align-items: center;
  width: 100%;
  height: 166rpx;
}

.level-one > view image {
  width: 68rpx;
  height: 68rpx;
  margin-left: 30rpx;
}

.level-one > view > view {
  margin-left: 50rpx;
}

.level-one > view .level-head {
  font-size: 30rpx;
  color: #8f722f;
}

.level-one > view .level-content {
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #b7a587;
}

.level-other {
  height: 180rpx;
  margin-top: 20rpx;
  white-space: nowrap;
}

.level-other .level-others:first-child {
  margin-left: 20rpx;
}

.level-others {
  position: relative;
  display: inline-block;
  width: 305rpx;
  height: 100%;
  margin-right: 20rpx;
}

.level-others > image {
  width: 100%;
  height: 100%;
}

.level-others > view {
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 176rpx;
}

.level-others > view > view {
  margin-left: 30rpx;
}

.level-others > view image {
  width: 68rpx;
  height: 68rpx;
  margin-right: 30rpx;
}

.level-others .other-head {
  font-size: 28rpx;
  color: #8f722f;
}

.level-others .other-content {
  color: #b7a587;
  width: 170rpx;
  font-size: 26rpx;
  margin-top: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
